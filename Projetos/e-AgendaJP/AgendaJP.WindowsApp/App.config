<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<connectionStrings>
		<!--SQL Server-->
		<add connectionString="Data Source=(LocalDb)\MSSqlLocalDB;Initial Catalog=e-AgendaJP;Integrated Security=True;Pooling=False"
			 name="SQLServer"
			 providerName="System.Data.SqlClient" />
		<!--SQLite-->
		<add connectionString="Data Source=..\..\..\AgendaJP.BD\dbo\SQLite\e-AgendaJP.db;Version=3"
			 name="SQLite"
			 providerName="System.Data.SQLite" />
	</connectionStrings>
	<appSettings>
		<!--Escolher tipo de banco-->
		<add key="bancoDeDados" value="SQLServer" />
	</appSettings>
</configuration>